{
  "name": "cypress_automation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "headTest": "npx cypress run --headed",
    "chromeHeadTest": "npx cypress run --headed --browser chrome",
    "chromeTest": "npx cypress run --browser chrome",
    "edgeTest": "npx cypress run --browser edge",
    "cloudTest": "npx cypress run --record --key 2ae76b08-7f92-4916-88ca-3af3c2c56d3e",
    "greenKartTest": "npx cypress run --spec \"cypress/integration/GreenKart/*.js\"",
    "greenKartCloudTest": "npx cypress run --record --key 2ae76b08-7f92-4916-88ca-3af3c2c56d3e --spec \"cypress/integration/GreenKart/*.js\"",
    "AutomationPracticeTest": "npx cypress run --spec \"cypress/integration/AutomationPractice/*.js\"",
    "AutomationPracticeCloudTest": "npx cypress run --record --key 2ae76b08-7f92-4916-88ca-3af3c2c56d3e --spec \"cypress/integration/AutomationPractice/*.js\"",
    "CalendarTest": "npx cypress run --spec \"cypress/integration/Calendar/*.js\"",
    "CalendarCloudTest": "npx cypress run --record --key 2ae76b08-7f92-4916-88ca-3af3c2c56d3e --spec \"cypress/integration/Calendar/*.js\"",
    "End_To_EndTest": "npx cypress run --spec \"cypress/integration/End_To_End/*.js\"",
    "End_To_EndCloudTest": "npx cypress run --record --key 2ae76b08-7f92-4916-88ca-3af3c2c56d3e --spec \"cypress/integration/End_To_End/*.js\""
  },
